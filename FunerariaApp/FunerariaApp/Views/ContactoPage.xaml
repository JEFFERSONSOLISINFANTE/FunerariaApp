<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FunerariaApp.Views.ContactoPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Nombres" x:Name="nombres" TextColor="#FFF" Background="#000"/>
            <Entry Placeholder="Correo" x:Name="Correo" TextColor="#FFF" Background="#000"/>
            <Entry Placeholder="Teléfono" x:Name="Telefono" TextColor="#FFF" Background="#000"/>
            <Entry Placeholder="Consulta" x:Name="Consulta" TextColor="#FFF" Background="#000"/>
            <Button Text="Enviar" x:Name="btnGuardar"
            Clicked="btnGuardar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>